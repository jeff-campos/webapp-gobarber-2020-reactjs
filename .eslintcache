index.tsx",["101"],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/pages/SignUp/styles.tsx",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/utils/getValidationErrors.ts",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/components/Button/styles.tsx",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/components/Input/styles.tsx",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/components/Tooltip/index.tsx",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/components/Tooltip/styles.tsx",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/context/AuthContex.tsx",["102","103"],"import React, { createContext, useCallback, useState, useContext } from 'react';\nimport api from '../services/api';\n\n/**\n * Interfaces\n */\n\ninterface AuthState {\n  token: string;\n  user: any;\n}\n\ninterface SignInCredentials {\n  email: string;\n  password: string;\n}\n\ninterface AuthContextData {\n  user: any;\n  signIn(credentials: SignInCredentials): Promise<void>;\n  signOut(): void;\n}\n\n/**\n * Criando o contexto\n */\n\nconst AuthContext = createContext<AuthContextData>({} as AuthContextData);\n\n/**\n * Criando um Hook de autenticação\n */\n\nfunction useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  if (!context) {\n    throw new Error('useAuth must be used within an AuthProvider');\n  }\n\n  return context;\n}\n\n/**\n * Criando o Provider de autenticação\n */\n\nconst AuthProvider: React.FC = ({ children }) => {\n  const [data, updateData] = useState<AuthState>(() => {\n    const token = localStorage.getItem('@GoBarber:token');\n    const user = localStorage.getItem('@GoBarber:user');\n\n    if (token && user) {\n      return { token, user: JSON.parse(user) };\n    }\n\n    return {} as AuthState;\n  });\n\n  const signIn = useCallback(async ({ email, password }) => {\n    const response = await api.post('sessions', {\n      email,\n      password,\n    });\n\n    const { token, user } = response.data;\n\n    localStorage.setItem('@GoBarber:token', token);\n    localStorage.setItem('@GoBarber:user', JSON.stringify(user));\n\n    updateData({ token, user });\n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@GoBarber:token');\n    localStorage.removeItem('@GoBarber:user');\n\n    updateData({} as AuthState);\n  }, []);\n\n  return (\n    <AuthContext.Provider value={{ user: data.user, signIn, signOut }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nexport { AuthProvider, useAuth };\n","/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/services/api.ts",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/components/ToastContainer/index.tsx",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/components/ToastContainer/styles.ts",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/context/index.tsx",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/context/ToastContext.tsx",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/components/ToastContainer/Toast/index.tsx",["104"],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/components/ToastContainer/Toast/styles.ts",[],"/Users/admin/Documents/_jobs/Estudos/2020/GoBarber/webapp-gobarber-reactjs/src/routes/index.tsx",[],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":17,"column":49,"nodeType":"117","messageId":"118","endLine":17,"endColumn":52,"suggestions":"119"},{"ruleId":"115","severity":1,"messag